dev_mode: false

api_endpoint:
  protocol: http
  hostname: 192.168.5.105
  port: 8083

http_client:
  timeout: 30
  max_conn: 100
  dial_timeout: 30
  idle_conn_timeout: 90

redis:
  host: 
    - localhost
  port: 6379
  DB: 1
  timeout: 15

portal_server:
  server_port: 9030
  log_level: debug
  log_dir: /path/to/logs/qxp-web
  templates_dir: /path/to/qxp-web/dist/templates
  # enable log to stdout
  enable_stdout: false
